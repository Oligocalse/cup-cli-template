{
  "private": true,
  "scripts": {
    "start": "cross-env API_ENV=development umi dev",
    "build:dev": "cross-env API_ENV=dev umi build",
    "build:stable": "cross-env API_ENV=stable umi build",
    "build": "cross-env API_ENV=dev umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.0.1",
    "@umijs/preset-react": "^1.3.0",
    "@umijs/preset-ui": "^2.0.9",
    "@umijs/test": "^3.2.6",
    "antd": "^4.0.0",
    "callapp-lib": "^2.1.8",
    "compressing": "^1.5.1",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.0.7",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-use": "^15.3.0",
    "swiper": "^6.5.1",
    "umi": "^3.2.6",
    "yorkie": "^2.0.0"
  }
}
